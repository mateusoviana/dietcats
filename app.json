{
  "expo": {
    "name": "DietCats",
    "slug": "dietcats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dietcats_icon.png",
    "userInterfaceStyle": "light",
    "scheme": "dietcats",
    "splash": {
      "image": "./assets/images/dietcats.png",
      "resizeMode": "cover",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dietcats.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dietcats_icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.dietcats.app"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-image-picker",
      ["expo-notifications", { "color": "#ffffff" }]
    ]
  }
}
